{
  "name": "prm-gp2gp-dashboard",
  "private": true,
  "description": "GP Registrations Data Platform",
  "version": "1.0.0",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "test": "jest",
    "e2e": "./node_modules/.bin/cypress run",
    "e2e:open": "./node_modules/.bin/cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 e2e",
    "test:e2e:open": "start-server-and-test develop http://localhost:8000 e2e:open",
    "test:e2e:ci": "CYPRESS_BASE_URL=http://localhost:9000 start-server-and-test serve http://localhost:9000 e2e",
    "format": "prettier --write \"**/*.{js,jsx,json,scss,md,ts,tsx}\"",
    "getPracticeMetrics": "node scripts/get-s3-data.js --datatype practiceMetrics",
    "getNationalMetrics": "node scripts/get-s3-data.js --datatype nationalMetrics",
    "getMetadata": "node scripts/get-s3-data.js --datatype metadata"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "pre-push": "npm run build && npm run test:e2e:ci"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,scss,md,ts,tsx}": "prettier --write"
  },
  "dependencies": {
    "@types/react-helmet": "^6.1.0",
    "aws-sdk": "^2.864.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.19.0",
    "gatsby": "^2.32.11",
    "gatsby-plugin-layout": "^1.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sass": "^2.8.0",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-json": "^2.11.0",
    "js-search": "^2.0.0",
    "lodash": "^4.17.21",
    "nhsuk-frontend": "^4.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-autosuggest": "^10.1.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.16.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.10",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/dom": "^7.30.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^4.0.1",
    "@testing-library/user-event": "^12.8.3",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/jest-when": "^2.7.2",
    "@types/moxios": "^0.4.10",
    "@types/react-dom": "^17.0.2",
    "axe-core": "^4.1.3",
    "babel-jest": "^25.5.1",
    "babel-preset-gatsby": "^0.2.36",
    "cypress": "^6.7.1",
    "cypress-axe": "^0.12.2",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "jest-when": "^3.2.1",
    "lint-staged": "^10.5.4",
    "moxios": "^0.4.0",
    "prettier": "1.19.1",
    "react-test-renderer": "^16.14.0",
    "start-server-and-test": "^1.12.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
